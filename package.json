{
  "name": "travel-time-webapp",
  "version": "0.1.0",
  "private": true,
  "description": "Web app to display travel time/routes on XRE device via deep link parameters using Google Maps APIs.",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "lint": "eslint src --ext .js",
    "build": "babel src --out-dir dist --source-maps",
    "build:watch": "babel src --out-dir dist --source-maps --watch",
    "serve:built": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.28.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
