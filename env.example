# Environment variables for Travel Time Web App
# Copy this file to .env and fill in your values

# Google Maps API Key (required)
MAPS_API_KEY=your_google_maps_api_key_here

# Alternative environment variable name (fallback)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Application settings
NODE_ENV=production
PORT=3000
APP_VERSION=0.1.0

# Logging
LOG_LEVEL=info

# Rate limiting
RATE_LIMIT_MAX=30

# Cache settings
FOOD_CACHE_TTL_MS=60000

# Debug settings
PLACES_DEBUG=0
MOCK_MODE=false

# Optional: Public base URL (auto-detected if not set)
# PUBLIC_BASE_URL=https://your-app.onrender.com

# Optional: Google Cloud Project (if using GCP services)
# GOOGLE_CLOUD_PROJECT=your-gcp-project-id
